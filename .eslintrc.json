{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "react/jsx-filename-extension": "off",
        "camelcase": 0,
        "import/no-unresolved": 0,
        "no-unused-vars": [
            "warn",
            {
                "args": "none"
            }
        ]
    }
}